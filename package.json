{
  "name": "kruti-icon-library",
  "version": "1.4.2",
  "description": "React icon library with 24px icons and dynamic sizing",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "generate-icons": "tsx scripts/generate-icons.ts",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "npm run generate-icons && npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "24px",
    "typescript",
    "kruti",
    "dynamic-sizing"
  ],
  "author": "Kruti",
  "license": "OLA",
  "repository": {
    "type": "git",
    "url": "https://github.com/akshayborade09/kruti-icon-library"
  }
}
